{
  "name": "redblue-gatsby",
  "private": true,
  "version": "0.1.0",
  "dependencies": {
    "@mdi/js": "latest",
    "@mdi/react": "latest",
    "bootstrap": "latest",
    "bootstrap.native": "latest",
    "dotenv": "latest",
    "gatsby": "latest",
    "gatsby-image": "latest",
    "gatsby-plugin-categories": "latest",
    "gatsby-plugin-draft": "latest",
    "gatsby-plugin-feed": "latest",
    "gatsby-plugin-load-script": "latest",
    "gatsby-plugin-react-helmet": "latest",
    "gatsby-plugin-scroll-reveal": "^0.0.4",
    "gatsby-plugin-sharp": "latest",
    "gatsby-plugin-smoothscroll": "latest",
    "gatsby-plugin-tags": "latest",
    "gatsby-remark-autolink-headers": "latest",
    "gatsby-remark-copy-linked-files": "latest",
    "gatsby-remark-images": "latest",
    "gatsby-remark-katex": "latest",
    "gatsby-remark-prismjs": "latest",
    "gatsby-remark-smartypants": "latest",
    "gatsby-source-filesystem": "latest",
    "gatsby-source-github-api": "latest",
    "gatsby-transformer-remark": "latest",
    "gatsby-transformer-sharp": "latest",
    "katex": "latest",
    "pangu": "latest",
    "prismjs": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-helmet": "latest",
    "referrer-policy": "latest"
  },
  "devDependencies": {
    "babel-eslint": "latest",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-config-prettier": "latest",
    "eslint-import-resolver-webpack": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "prettier": "latest",
    "prettier-eslint-cli": "latest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-merge": "lint-staged",
      "pre-rebase": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "prettier-eslint",
      "git add"
    ],
    "*.+(css)": [
      "prettier --write",
      "git add"
    ]
  },
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "lint": "eslint \"src/**/*.+(js|jsx)\"",
    "fix": "eslint --fix \"src/**/*.+(js|jsx)\"",
    "format": "prettier --write \"**/*.+(js|jsx|css)\"",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  }
}

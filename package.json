  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "lint": "eslint \"src/**/*.+(js|jsx)\"",
    "fix": "eslint --fix \"src/**/*.+(js|jsx)\"",
    "format": "prettier --write \"**/*.+(js|jsx|css)\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""

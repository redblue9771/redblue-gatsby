{
  "name": "redblue-gatsby",
  "private": true,
  "version": "0.1.1",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "apollo-boost": "^0.4.9",
    "bootstrap": "^4.5.0",
    "clsx": "^1.1.1",
    "dotenv": "latest",
    "gatsby": "^2.23.0",
    "gatsby-image": "^2.4.6",

    "react-helmet": "^6.0.0",
    "react-horizontal-scrolling-menu": "latest",
    "referrer-policy": "latest"
  },
  "devDependencies": {
    "babel-eslint": "latest",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "latest",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "prettier": "^2.0.5",
    "prettier-eslint-cli": "latest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "prettier-eslint",
      "git add"
    ],
    "*.+(css)": [
      "prettier --write",
      "git add"
    ]
  },
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "lint": "eslint \"src/**/*.+(js|jsx)\"",
    "fix": "eslint --fix \"src/**/*.+(js|jsx)\"",
    "format": "prettier --write \"**/*.+(js|jsx|css)\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  }
}
